FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     interPlicFoam;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         6.0;

deltaT          0.001;

writeControl    adjustable;

writeInterval   1.0;

purgeWrite      0;

writeFormat     binary;

writePrecision  9;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           2.0;
maxAlphaCo      0.5;

maxDeltaT       1;
